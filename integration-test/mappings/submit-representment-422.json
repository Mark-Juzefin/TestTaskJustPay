{
  "priority": 1,
  "request": {
    "method": "POST",
    "url": "/api/v1/dispute-representments/create",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "equalToJson": "{ \"order_id\": \"file-not-ready-error\" }",
        "ignoreExtraElements": true,
        "ignoreArrayOrder": true
      }
    ]
  },
  "response": {
    "status": 422,
    "jsonBody": {
      "error": {
        "code": "2.01",
        "message": "File not ready"
      }
    }
  }
}